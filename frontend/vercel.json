{
  "version": 2,
  "public": true,
  "name": "poprank",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": [
      "dist/frontend/server/**",
      "dist/frontend/browser/**.js",
      "dist/frontend/browser/**.css",
      "dist/frontend/browser/**.ico",
      "dist/frontend/browser/**.png"
    ]
    }
  }
}
